/* carroussel */
.region-sidebar-first .jcarousel-container.jcarousel-container-horizontal {
    height: 74px;
    margin: auto;
    padding: 0px 20px;
    width: 114px;
}
.region-content-top .jcarousel-container.jcarousel-container-horizontal {
    height: 163px;
    margin: auto;
    padding: 0px 30px;
    width: 570px;
}

.region-sidebar-first .jcarousel-container .jcarousel-clip-horizontal {
    width: 114px;
}
.region-content-top .jcarousel-container .jcarousel-clip-horizontal {
    width: 570px;
}

.region-sidebar-first .jcarousel-container .jcarousel-item {
    background: none repeat scroll 0 0 #FFFFFF;
    height: 74px;
    list-style: none outside none;
    overflow: hidden;
    padding: 0;
    width: 114px;
}
.region-content-top .jcarousel-container .jcarousel-item {
    height: 163px;
    list-style: none outside none;
    overflow: hidden;
    padding: 0;
    width: 542px;
}

.jcarousel-container .jcarousel-item-horizontal {
    margin: 0;
}
.jcarousel-container .jcarousel-item-placeholder {
    background: url("throbber.gif") no-repeat scroll center center #FFFFFF;
    color: #000000;
}
.jcarousel-container .jcarousel-next, .jcarousel-container .jcarousel-prev {
    background-image: url("../images/visionneuse/arrows.png");
    display: block;
    height: 12px;
    width: 7px;
}

.region-sidebar-first .jcarousel-container .jcarousel-prev-horizontal {
    background-position: -7px 0;
    left: 5px;
    position: absolute;
    top: 30px;
}
.region-sidebar-first .jcarousel-container .jcarousel-prev-horizontal:hover {
    background-position: -21px 0;
}
.region-sidebar-first .jcarousel-container .jcarousel-prev-disabled-horizontal, .jcarousel-container .jcarousel-prev-disabled-horizontal:hover, .jcarousel-container .jcarousel-prev-disabled-horizontal:active {
    background-position: -7px 0;
    cursor: default;
}
.region-content-top .jcarousel-container .jcarousel-prev-horizontal {
    background-position: -35px 0;
    left: 7px;
    position: absolute;
    top: 65px;
}
.region-content-top .jcarousel-container .jcarousel-prev-horizontal:hover {
    background-position: -21px 0;
}
.region-content-top .jcarousel-container .jcarousel-prev-disabled-horizontal, .jcarousel-container .jcarousel-prev-disabled-horizontal:hover, .jcarousel-container .jcarousel-prev-disabled-horizontal:active {
    background-position: -35px 0;
    cursor: default;
}

.region-sidebar-first .jcarousel-container .jcarousel-next-horizontal {
    background-position: 0 -22px;
    position: absolute;
    right: 5px;
    top: 30px;
}
.region-sidebar-first .jcarousel-container .jcarousel-next-horizontal:hover {
    background-position: -14px -22px;
}
.region-sidebar-first .jcarousel-container .jcarousel-next-disabled-horizontal, .jcarousel-container .jcarousel-next-disabled-horizontal:hover, .jcarousel-container .jcarousel-next-disabled-horizontal:active {
    background-position: 0 -22px;
    cursor: default;
}
.region-content-top .jcarousel-container .jcarousel-next-horizontal {
    background-position: -28px -22px;
    position: absolute;
    right: 7px;
    top: 65px;
}
